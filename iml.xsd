<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="https://www.w3schools.com" xmlns="https://www.w3schools.com" elementFormDefault="qualified">



    <xsd:complexType name="complexSongs">
        <xsd:sequence>
            <xsd:element name="Anio" type="simpleTypeAnio"/>
            <xsd:element name="Pais" type= "complexTypePais" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="simpleTypeAnio">
        <xsd:restriction base="xsd:integer">        
            <xsd:minInclusive value="1900"/>
            <xsd:maxInclusive value="2018"/>
        </xsd:restriction>    
    </xsd:simpleType>

    <xsd:complexType name="complexTypePais">
        <xsd:sequence>
            <xsd:element name="Disco" type="complexTypeDisco" maxOccurs = "unbounded"/>   
        </xsd:sequence>
        <xsd:attribute name="pais" type="xsd:string"/>
        <xsd:attribute name="lang" type="simpleTypeAttlang"/>        
    </xsd:complexType>

    <xsd:simpleType name="simpleTypeAttlang">
        <xsd:restriction base="xsd:string">        
        <xsd:pattern value="[a-z][a-z]"/>
        </xsd:restriction>    
    </xsd:simpleType>

    <xsd:complexType name="complexTypeDisco">    
        <xsd:sequence>
            <xsd:element name="Titulo" type="xsd:string"/>
            <xsd:element name="Premios" type="complexTypePremios"/>
            <xsd:element name ="Interprete" type="xsd:string"/>
            <xsd:element name ="Cancion" type="complexTypeCancion"/>
        </xsd:sequence>
        <xsd:attribute name="idd" type="simpleTypeAttidd"/>
        <xsd:attribute name="langs" type="simpleTypeAttlangs"/>    
    </xsd:complexType>

    <xsd:simpleType name="simpleTypeAttidd">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9][0-9][0-9][0-9]-[0-9][0-9][0-9]-[0-9][0-9][0-9]"/>
        </xsd:restriction>   
    </xsd:simpleType>

    <xsd:simpleType name="simpleTypeAttlangs">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="([a-z][a-z] )*"/>
        </xsd:restriction>    
    </xsd:simpleType>

    <xsd:complexType name="complexTypePremios">
        <xsd:sequence>
            <xsd:element name="Premio" type="simpleTypePremio"/>
        </xsd:sequence>                                                    
    </xsd:complexType>
            
    <xsd:simpleType name="simpleTypePremio">
        <xsd:restriction base="xsd:string">        
            <xsd:enumeration value="DiscodeOro"/>
            <xsd:enumeration value="Grammy"/>
            <xsd:enumeration value="LamparaMinera"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="complexTypeCancion">    
        <xsd:sequence>
            <xsd:element name="Titulo" type="xsd:string"/>
            <xsd:element name="Duracion" type="simpleTypeDuracion"/>
            <xsd:element name="Generos" type="complexTypeGeneros"/>
            <xsd:element name="Version" type="complexTypeVersion"/>
        </xsd:sequence>
        <xsd:attribute name="idc" type="simpleTypeAttidc"/>                                                    
    </xsd:complexType>

    <xsd:simpleType name="simpleTypeAttidc">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9][0-9][0-9][0-9]-[0-9][0-9][0-9]-[0-9][0-9][0-9]-[0-9][0-9]"/>
        </xsd:restriction>   
    </xsd:simpleType>

    <xsd:simpleType name="simpleTypeDuracion">
        <xsd:restriction base="xsd:integer">                                            
            <xsd:minExclusive value="0"/>
            <xsd:maxExclusive value="600"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="complexTypeGeneros">
        <xsd:sequence>
            <xsd:element name="Genero" type="simpleTypeGenero"/>
        </xsd:sequence>                                                    
    </xsd:complexType> 

    <xsd:simpleType name="simpleTypeGenero">    
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Rock"/>
            <xsd:enumeration value="Pop"/>
            <xsd:enumeration value="Country"/>
            <xsd:enumeration value="Balada"/>
        </xsd:restriction>
    </xsd:simpleType>


    <xsd:complexType name="complexTypeVersion">    
        <xsd:sequence>       
            <xsd:element name="Titulo" type="xsd:string"/>
            <xsd:element name="Idc" type="xsd:string"/>
            <xsd:element name="IML" type="xsd:string"/>
        </xsd:sequence>
        <xsd:attribute name="anio" type="xsd:string"/>    
    </xsd:complexType>
    <xsd:element name="Songs" type="complexSongs"></xsd:element>
</xsd:schema>        
        
